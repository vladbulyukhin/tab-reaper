{
  "name": "tabs-reaper",
  "version": "1.0.0",
  "description": "",
  "source": "src/manifest.json",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "parcelIgnore": [
    "www.flaticon.com"
  ],
  "scripts": {
    "build": "parcel build src/manifest.json",
    "ci": "npm ci",
    "i": "npm install",
    "lint": "eslint {src,tests}/**/*.{js,ts,tsx}",
    "lint:fix": "npm run lint -- --fix",
    "start": "parcel watch src/manifest.json --host localhost --config @parcel/config-webextension",
    "test": "karma start karma.conf.js",
    "test:ci": "npm run test -- --single-run --browsers ChromeHeadless"
  },
  "keywords": [],
  "author": "Vladislav Bulyukhin <vladislav.bulyukhin@gmail.com>",
  "license": "GPL-3.0",
  "devDependencies": {
    "@parcel/config-webextension": "^2.8.3",
    "@types/chrome": "^0.0.235",
    "@types/jasmine": "^4.3.1",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine-core": "^4.6.0",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-cli": "^2.0.0",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "karma-typescript": "^5.5.4",
    "parcel": "^2.8.3",
    "parcel-resolver-ignore": "^2.1.3",
    "prettier": "^2.8.8"
  }
}
