{
  "manifest_version": 3,
  "name": "Tab Reaper: Unused Tab Collector [DEVELOPMENT BUILD]",
  "description": "Automatically closes tabs that haven't been used for a while. [THIS EXTENSION IS FOR TESTING]",
  "version": "0.0.1",
  "permissions": ["tabs", "storage"],
  "icons": {
    "16": "./icons/icon-active-16.png",
    "32": "./icons/icon-active-32.png",
    "96": "./icons/icon-active-96.png"
  },
  "action": {
    "default_title": "Tab Reaper"
  },
  "background": {
    "service_worker": "background.ts",
    "type": "module"
  },
  "options_ui": {
    "page": "./options.html",
    "open_in_tab": false
  }
}
